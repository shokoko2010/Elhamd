{
  "version": 2,
  "name": "elhamd-imports",
  "builds": [
    {
      "src": "server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.ts"
    }
  ],
  "env": {
    "NEXTAUTH_URL": "https://elhamdimports.com",
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NEXTAUTH_URL": "https://elhamdimports.com",
      "NODE_ENV": "production"
    }
  },
  "functions": {
    "server.ts": {
      "maxDuration": 30
    }
  },
  "crons": []
}